    "jest": "^29.2.2",
    "jest-environment-jsdom": "^29.2.2",
    "jest-location-mock": "^1.0.9",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^13.1.0",
    "prettier": "^2.7.1",
    "react-hooks": "^1.0.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
