    "@react-three/drei": "^9.80.2",
    "@react-three/fiber": "^8.13.6",
    "@types/three": "^0.155.0",
    "axios": "^1.1.3",
    "chart.js": "^4.3.0",
    "chartjs-plugin-annotation": "^3.0.1",
    "emoji-regex": "^10.2.1",
    "eventemitter3": "^5.0.0",
    "husky": "^8.0.3",
    "lodash": "^4.17.21",
